[global_config]
  geometry_hinting = True
[keybindings]
  zoom_in = <Primary>equal
  go_up = <Primary><Alt>Up
  go_down = <Primary><Alt>Down
  go_left = <Primary><Alt>Left
  go_right = <Primary><Alt>Right
  split_horiz = <Primary>underscore
  split_vert = <Primary>bar
[profiles]
  [[default]]
    background_darkness = 0.95
    background_type = transparent
    cursor_fg_color = "#08052b"
    cursor_bg_color = "#00ff00"
    foreground_color = "#00ff00"
    scrollback_lines = 495
    scrollback_infinite = True
    bold_is_bright = True
    title_hide_sizetext = True
{{- if eq .chezmoi.osRelease.id "endeavouros" }}
    login_shell = True
    background_color = "#08052b"
    palette = "#08052b:#ff7f7f:#7f3fbf:#cc3980:#7fbaff:#7f3fbf:#7f7fff:#e3e3ea:#7fbaff:#ff7f7f:#9999cc:#ff7f7f:#7f7fff:#7f3fbf:#7f7fff:#e3e3ea"
    title_transmit_fg_color = "#000000"
    title_transmit_bg_color = "#7f3fbf"
    title_receive_bg_color = "#7f7fff"
    title_inactive_bg_color = "#77767b"
{{- else }}
    title_transmit_fg_color = "#000000"
    title_transmit_bg_color = "#aa0000"
    title_receive_bg_color = "#2ec27e"
    title_inactive_bg_color = "#77767b"
{{- end }}
